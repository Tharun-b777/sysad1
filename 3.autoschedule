#!/bin/bash 
var=$(date '+%Y-%m-%d')
for i in {1..50};do
grep $var /home/tharun/Desktop/sysad1/position.log | grep -w "Army${i}" | cut -f 1,4,5 -d' ' > /home/Army${i}/post_alloted
